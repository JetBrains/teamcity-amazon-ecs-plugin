<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>aws-ecs-plugin</name>
        <display-name>Amazon Elastic Container Service Support</display-name>
        <version>@Plugin_Version@</version>
        <description>Allows running TeamCity build agents on Amazon Elastic Container Service</description>
        <vendor>
            <name>JetBrains, s.r.o.</name>
            <url>https://www.jetbrains.com</url>
        </vendor>
    </info>
    <deployment use-separate-classloader="true"/>
</teamcity-plugin>
